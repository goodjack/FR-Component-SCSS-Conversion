{
  "name": "uniqlo-ui",
  "version": "1.0.1",
  "description": "uniqlo Design UI components built with React",
  "main": "./lib",
  "scripts": {
    "lint": "npm run lint:eslint -s && npm run lint:jscs -s",
    "test": "npm run test:karma",
    "build": "babel ./src --out-dir ./lib",
    "test:karma": "npm run test:base -- --single-run",
    "test:watch": "npm run test:base -- --auto-watch",
    "test:base": "./node_modules/.bin/karma start",
    "test:mocha": "cross-env NODE_ENV=test mocha --reporter progress --compilers js:babel-core/register,js:babel-polyfill --recursive \"./test/**/*Spec.js\"",
    "lint:eslint": "eslint --ext .jsx,.js src/ test/",
    "lint:jscs": "jscs -v src/ test/",
    "watch:lint": "watch 'npm run lint -s' src/ test/",
    "prebuild": "rimraf lib",
    "prepublish": "npm run build"
  },
  "keywords": [
    "react",
    "react-component",
    "uniqlo design",
    "uniqlo-ui",
    "uniqlo ui"
  ],
  "author": "Fast Retailing Co., Ltd.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/fastretailing/fr-webcms-components"
  },
  "dependencies": {
    "fetch-jsonp": "1.0.0",
    "inline-style-prefixer": "^0.5.2",
    "lodash.merge": "^3.3.2",
    "lodash.throttle": "~3.0.4",
    "react-addons-create-fragment": "^0.14.0",
    "react-addons-pure-render-mixin": "^0.14.0",
    "react-addons-transition-group": "^0.14.0",
    "react-addons-update": "^0.14.0",
    "superagent": "1.7.2",
    "warning": "^2.1.0"
  },
  "peerDependencies": {
    "react": "^0.14.0",
    "react-dom": "^0.14.0",
    "react-tap-event-plugin": "^0.2.0"
  },
  "devDependencies": {
    "babel": "^6.3.26",
    "babel-cli": "^6.4.5",
    "babel-core": "^6.4.5",
    "babel-eslint": "4.1.8",
    "babel-loader": "6.2.2",
    "babel-plugin-add-module-exports": "0.1.2",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "6.3.13",
    "babel-preset-es2016-node5": "1.1.2",
    "babel-preset-react": "6.5.0",
    "babel-preset-stage-0": "^6.3.13",
    "babel-preset-stage-3": "^6.3.13",
    "babel-register": "6.4.3",
    "babelify": "7.2.0",
    "browserify-istanbul": "^0.2.1",
    "chai": "3.5.0",
    "cross-env": "1.0.7",
    "css-loader": "0.23.1",
    "eslint": "1.10.3",
    "eslint-config-airbnb": "4.0.0",
    "eslint-plugin-babel": "^3.0.0",
    "eslint-plugin-import": "0.12.1",
    "eslint-plugin-react": "3.16.1",
    "gulp": "^3.9.0",
    "gulp-eslint": "^1.0.0",
    "gulp-if": "^2.0.0",
    "jscs": "2.9.0",
    "karma": "^0.13.3",
    "karma-browserify": "^4.2.1",
    "karma-chai-sinon": "^0.1.5",
    "karma-coverage": "^0.5.3",
    "karma-mocha": "^0.2.0",
    "karma-mocha-reporter": "^1.0.4",
    "karma-phantomjs-launcher": "^0.2.0",
    "mocha": "^2.2.5",
    "node-sass": "3.4.2",
    "phantomjs": "^1.9.17",
    "postcss-loader": "0.8.2",
    "react": "^0.14.0",
    "react-addons-test-utils": "^0.14.3",
    "react-dom": "^0.14.0",
    "react-hot-loader": "^1.3.0",
    "react-tap-event-plugin": "^0.2.0",
    "recursive-readdir-sync": "^1.0.6",
    "rimraf": "^2.4.3",
    "sass-loader": "3.2.0",
    "sinon": "1.17.3",
    "sinon-chai": "^2.8.0",
    "style-loader": "0.13.1",
    "webpack": "^1.12.9"
  }
}
